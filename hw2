    <script>
        
        var number=[]
        function generate(){
            var box = document.querySelectorAll('.select')

            box = Array.from(box)
           //console.log(box)
            var checked = box.filter(function(item){
                return item.checked
            })
            
            var box2 = document.querySelectorAll('.choose')
            box2 = Array.from(box2)
             var summ = box2.filter(function(item){
                return item.checked
            })
             
             for(var i =0 ; i < summ.length;i++){
                 console.log(summ[i].value)
             }
    //

            var checkif
            var number=[]
            for(var i = 0 ; i<10000 ;i++){
         
                str = i+"";
                //console.log(str);
                checkif = false
                for(var j = 0 ; j<checked.length;j++){

                    if(str.indexOf(checked[j].value)!=-1){
                        checkif = true
                    }
                }
                if(checkif ==false){
                    number.push(i)
                }
            } 
//            createTable(number)
   
     ////////////////////////////////////////////////      
            var result=[]
            var num
            var check2
            var s
            var arr
            for(var k = 0 ; k<number.length;k++){
                
                str = k+""
                
                do{
                    arr = str.split();
                    s = 0
                    for(var j = 0;j<arr.length;j++){
                        s+=parseInt(arr[j]);
                    }
                }while(s>9)
                    check2=false
                for(var i = 0 ; i< summ.length;i++){
                     if(s!=summ[i]){
                         check2 = true
                     }
                    
                 }
            }
            createTable(result)
        }

            
        
        function createTable(data){
            var table =document.createElement('table')
            var output = document.getElementById('output')
            output.innerHTML =""
            table.className = 'table'

            //tr,td
            var tr ,td

            var k=0
            for(var i = 0 ;i < data.length ;i++){
                tr = document.createElement('tr')
                
                for(var j = 0 ; j< 5;j++){
                    td = document.createElement('td')
                if(data[k]==undefined){
                    break;
                }
                    td.innerHTML= data[k++]
                    tr.appendChild(td)
                }
                table.appendChild(tr)
            }
        
            output.appendChild(table)
            
        }
        
        
        
    </script>
    
    
